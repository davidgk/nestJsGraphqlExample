{
  "name": "graphqlexample",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docker-compose:api-up:local": "docker-compose --env-file .env.local up -d --no-recreate api",
    "docker-compose:api-stop:local": "docker-compose --env-file .env.local stop api",
    "docker-compose:db-up:local": "docker-compose --env-file .env.local up -d --no-recreate postgres",
    "docker-compose:db-stop:local": "docker-compose --env-file .env.local stop",
    "docker-compose:down:local": "docker-compose --env-file .env.local down",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm:cli": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -d ./src/database/typeorm.config-migrations.ts",
    "migration:generate": "npm run typeorm:cli migration:generate",
    "migration:create": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:create",
    "migration:run:test": "NODE_ENV=test npm run typeorm:cli migration:run",
    "migration:run:dev": "NODE_ENV=dev npm run typeorm:cli migration:run",
    "migration:run:local": "NODE_ENV=local npm run typeorm:cli migration:run",
    "migration:revert:test": "NODE_ENV=test npm run typeorm:cli migration:revert",
    "migration:revert:dev": "NODE_ENV=dev npm run typeorm:cli migration:revert",
    "migration:revert": "npm run typeorm:cli migration:revert",
    "start": "nest start",
    "start:local": "NODE_ENV=local nest start",
    "start:debug:local": "NODE_ENV=local nest start --debug --watch",
    "start:debug:dev": "NODE_ENV=dev nest start --debug --watch",
    "start:debug:stage": "NODE_ENV=stage nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:cov": "jest --config jest.config.js --coverage",
    "test:cov:test": "NODE_ENV=test jest --config jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "husky install"
  },
  "dependencies": {
    "@adminjs/express": "5.0.1",
    "@adminjs/nestjs": "5.0.1",
    "@adminjs/typeorm": "4.0.0",
    "@golevelup/ts-jest": "0.3.3",
    "@nestjs/common": "9.0.11",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.11",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.0.11",
    "@nestjs/swagger": "6.1.0",
    "@nestjs/typeorm": "9.0.1",
    "express-formidable": "1.2.0",
    "express-session": "1.17.3",
    "jwks-rsa": "^2.1.4",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "typeorm": "0.3.7",
    "uuid": "9.0.0",
    "joi": "17.6.0",
    "webpack": "5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.11",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.7",
    "@types/node": "18.7.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "community-faker": "5.5.5",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
